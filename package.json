{
  "name": "pingone-user-management",
  "version": "1.1.0",
  "private": true,
  "description": "Ping Identity User Management Application",
  "keywords": [
    "pingidentity",
    "user-management",
    "csv-import"
  ],
  "homepage": "https://github.com/curtismu7/pingone-sample-js",
  "main": "server/app.js",
  "scripts": {
    "start": "node server/app.js",
    "dev": "nodemon server/app.js",
    "test": "NODE_ENV=test jest --config=jest.config.js",
    "build": "cp node_modules/@ping-identity/p14c-js-sdk-auth/dist/browser/@ping-identity/p14c-js-sdk-auth.js p14c-js-sdk-auth.js",
    "version:patch": "npm version patch && git push && git push --tags",
    "version:minor": "npm version minor && git push && git push --tags",
    "version:major": "npm version major && git push && git push --tags",
    "release": "npm run build && npm run version:patch",
    "test:watch": "NODE_ENV=test jest --watch --config=jest.config.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@ping-identity/p14c-js-sdk-auth": "^1.0.0-pre.2",
    "axios": "^1.10.0",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "form-data": "^4.0.3",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "papaparse": "^5.5.3",
    "puppeteer": "^24.10.2",
    "winston": "^3.17.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "html-escaper": "^3.0.3",
    "jest": "^29.7.0",
    "mongo-sanitize": "^1.1.0",
    "nodemon": "^3.0.2",
    "supertest": "^7.1.1"
  },
  "ignore": [
    "**/.*",
    "node_modules"
  ]
}
